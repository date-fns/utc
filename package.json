{
  "name": "@date-fns/utc",
  "version": "1.1.1",
  "description": "UTC date utils",
  "scripts": {
    "test": "env TZ=Asia/Kolkata vitest",
    "build": "make build"
  },
  "sideEffects": false,
  "browser": "./index",
  "main": "./index.js",
  "module": "./index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      }
    },
    "./date": {
      "require": {
        "types": "./date/index.d.ts",
        "default": "./date/index.js"
      },
      "import": {
        "types": "./date/index.d.mts",
        "default": "./date/index.mjs"
      }
    },
    "./date/mini": {
      "require": {
        "types": "./date/mini.d.ts",
        "default": "./date/mini.js"
      },
      "import": {
        "types": "./date/mini.d.mts",
        "default": "./date/mini.mjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/date-fns/utc.git"
  },
  "keywords": [
    "date-fns",
    "Date",
    "UTC",
    "Time zones"
  ],
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/date-fns/utc/issues"
  },
  "homepage": "https://github.com/date-fns/utc#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "babel-plugin-add-import-extension": "^1.6.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
